@{
    ViewBag.Title = "Xác nhận quên mật khẩu";
}

<link rel="stylesheet" type="text/css" href="~/Content/assest/styles/bootstrap4/bootstrap.min.css">
<link href="~/Content/dangky.css" rel="stylesheet" />

<style>
    .news_section_container {
        margin-top: 30px;
    }

    .row-item-new {
        margin-bottom: 20px;
    }

    .title-news {
        font-size: 24px;
        border-bottom: 1px solid;
        padding: 15px 0;
        margin-bottom: 25px;
    }

    .img-box img {
        max-width: 100%;
        max-height: 100%;
    }
</style>
<div class="container news_section_container">
    <div class="row">
        <div class="col-md-12">
            <h1 class="title-news"> @ViewBag.Title.</h1>
            @*<h2>@ViewBag.Title.</h2>*@

            <p>
                Vui Lòng kiểm tra email để đặt lại mật khẩu.
            </p>
        </div>

    </div>

</div>



