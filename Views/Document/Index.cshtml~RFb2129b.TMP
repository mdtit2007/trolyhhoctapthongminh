@model IEnumerable<ungdunghocthongminh.Models.EF.Document>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>TaiLieu</title>
    <link href="~/Content/assest/css/tailieu.css" rel="stylesheet" />
</head>
<body>
    <div class="container">
        
        <!-- Sidebar -->
        @Html.Action("MenuLetf", "Menu")

        <!-- Main Content -->
        <div class="main-content">
            <div class="product-grid">
                @if (Model != null && Model.Any())
                {
                    foreach (var item in Model)
                    {
                        <!-- Product 1 -->
                        <div class="product-card">
                            <img src="@item.Img" alt="@item.Title">
                            <h4><a href="/chi-tiet/@item.Alias-p@(item.Id)">@item.Title</a></h4>
                        </div>
                    }
                }


            </div>
        </div>
    </div>
</body>
</html>
@section scripts{
    <script src="~/Content/assest/js/categories_custom.js"></script>

}